{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-LR255021-Streaming"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Odbc1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Odbc1'"
		},
		"Odbc1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Odbc1'"
		},
		"Odbc1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "AFP_2021_LR255021"
		},
		"integrationRuntime-ADF-Streaming_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1f037183-21d0-4c33-8356-296f23107c7c/resourcegroups/AFP-2021-MS/providers/Microsoft.DataFactory/factories/ADF-LR255021-EventHub/integrationruntimes/IR-ADF-LR255021-EventHub"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-LR255021-Streaming_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Odbc1_connectionString": {
						"value": "[parameters('Odbc1_connectionString')]"
					},
					"Odbc1_password": {
						"value": "[parameters('Odbc1_password')]"
					},
					"integrationRuntime-ADF-Streaming_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('integrationRuntime-ADF-Streaming_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"Odbc1_properties_typeProperties_userName": {
						"value": "[parameters('Odbc1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}